<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sii_certificate_password_view" model="ir.ui.view">
            <field name="name">sii.certificate.password.form</field>
            <field name="model">sii.certificate.password</field>
            <field name="arch" type="xml">
                <form string="Insert Certificate Password">
                    <group>
                        <field name="password" password="True" />
                    </group>
                    <footer>
                        <button name="action_decryt_keys" type="object" string="Process certificate" class="oe_highlight" />
                        <button string="Cancel" special="cancel" />
                    </footer>
                 </form>
            </field>
        </record>

    </data>
</odoo>
