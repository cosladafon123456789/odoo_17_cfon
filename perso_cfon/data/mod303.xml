            <field name="country_id">68</field>
            <field name="tax_negate">False</field>
        </record>

        <record id="balance_mod303_casilla_97_neg" model="account.account.tag">
            <field name="name">-mod303[97]</field>
            <field name="applicability">taxes</field>
            <field name="country_id">68</field>
            <field name="tax_negate">True</field>
        </record>

