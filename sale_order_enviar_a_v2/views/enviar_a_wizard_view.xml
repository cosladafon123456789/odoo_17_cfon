<odoo>
    <record id="view_enviar_a_wizard_form" model="ir.ui.view">
        <field name="name">enviar.a.wizard.form</field>
        <field name="model">enviar.a.wizard</field>
        <field name="arch" type="xml">
            <form string="Enviar A">
                <group>
                    <field name="location_id"/>
                </group>
                <footer>
                    <button string="Enviar" type="object" name="action_enviar_producto" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
