<odoo>
  <record id="view_cf_productivity_ticket_daily_tree_patch3" model="ir.ui.view">
    <field name="name">cf.productivity.ticket.daily.tree.patch3</field>
    <field name="model">cf.productivity.ticket.daily</field>
    <field name="arch" type="xml">
      <tree>
        <field name="user_id"/>
        <field name="date"/>
        <field name="msg_count"/>
        <field name="stage_changes"/>
        <field name="total_interactions"/>
      </tree>
    </field>
  </record>

  <record id="action_cf_productivity_ticket_daily_patch3" model="ir.actions.act_window">
    <field name="name">Productividad tickets</field>
    <field name="res_model">cf.productivity.ticket.daily</field>
    <field name="view_mode">tree</field>
  </record>

  <menuitem id="menu_cf_productivity_ticket_daily_patch3"
            name="Productividad tickets"
            parent="stock.menu_stock_root"
            action="action_cf_productivity_ticket_daily_patch3"
            groups="base.group_no_one"
            sequence="55"/>
</odoo>
