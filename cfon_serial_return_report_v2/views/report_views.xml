<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_cfon_lot_return_report_tree" model="ir.ui.view">
    <field name="name">cfon.lot_return_report.tree</field>
    <field name="model">cfon.lot_return_report</field>
    <field name="arch" type="xml">
      <tree string="SN/Lotes con 2+ devoluciones y stock">
        <field name="lot_id"/>
        <field name="return_count"/>
        <field name="on_hand_qty"/>
        <field name="alert"/>
      </tree>
    </field>
  </record>

  <record id="action_cfon_lot_return_report" model="ir.actions.act_window">
    <field name="name">SN/Lotes +2 devoluciones en stock</field>
    <field name="res_model">cfon.lot_return_report</field>
    <field name="view_mode">tree</field>
    <field name="context">{}</field>
  </record>

  <menuitem id="menu_cfon_lot_return_root" name="CFON Control" parent="stock.menu_traceability" sequence="24"/>
  <menuitem id="menu_cfon_lot_return_report"
            name="SN/Lotes en stock +2 dev"
            parent="menu_cfon_lot_return_root"
            action="action_cfon_lot_return_report"
            sequence="1"/>
</odoo>
