<odoo>
  <record id="view_order_form_send_to_warehouse" model="ir.ui.view">
    <field name="name">sale.order.form.send.to.warehouse</field>
    <field name="model">sale.order</field>
    <field name="inherit_id" ref="sale.view_order_form"/>
    <field name="arch" type="xml">
      <xpath expr="//button[@name='action_view_invoice']" position="after">
        <button name="action_open_send_to_warehouse_wizard"
                string="Enviar A"
                type="object"
                class="btn-primary"
                context="{'default_sale_order_id': active_id}" />
      </xpath>
    </field>
  </record>
</odoo>