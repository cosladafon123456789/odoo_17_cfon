<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_partner_property_form" model="ir.ui.view">
        <field name="name">res.partner.inherit.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="account.view_partner_property_form" />
        <field name="type">form</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_payable_id']" position="after">
                <field name="property_willpay_account_recieved_id" />
                <field name="property_willpay_account_collection_id" />
                <field name="property_willpay_account_discounted_id" />
                <field name="property_willpay_account_unpaid_id" />
                <field name="property_willpay_account_interests_id" />
                <field name="property_willpay_account_management_fee_id" />
            </xpath>
        </field>
    </record>

</odoo>