<odoo>
    <record id="view_stock_lot_form_inherit_imei_label" model="ir.ui.view">
        <field name="name">stock.lot.form.imei.label</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="print_imei_label"
                        type="object"
                        string="Imprimir Etiqueta IMEI"
                        class="btn-primary"
                        context="{'report_type': 'qweb-pdf'}"/>
            </xpath>
        </field>
    </record>
</odoo>
