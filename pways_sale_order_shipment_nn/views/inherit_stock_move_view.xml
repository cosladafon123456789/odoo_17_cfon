<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_stock_move_form_view" model="ir.ui.view">
            <field name="name">inherit.stock.move.from.view</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/field[@name='date_deadline']" position="after">
                    <field name="imei_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
