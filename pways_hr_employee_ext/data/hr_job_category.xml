<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="hr_job_category01" model="hr.job.category">
			<field name="name">Manager</field>
		</record>
		<record id="hr_job_category02" model="hr.job.category">
			<field name="name">Semi-Skilled</field>
		</record>
		<record id="hr_job_category03" model="hr.job.category">
			<field name="name">Skilled</field>
		</record>

        <!-- Action server -->
        <record id="action_approve_contract" model="ir.actions.server">
            <field name="name">Hr Contract Approved</field>
            <field name="model_id" ref="hr_contract.model_hr_contract"/>
            <field name="state">code</field>
            <field name="code">records.action_approve()</field>
        </record>     
        <record id="action_reject_contract" model="ir.actions.server">
            <field name="name">Hr Contract Reject</field>
            <field name="model_id" ref="hr_contract.model_hr_contract"/>
            <field name="state">code</field>
            <field name="code">records.action_reject()</field>
        </record>        
	</data>
</odoo>

